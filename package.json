{
  "name": "baas-admin-sdk",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/10gen/baas-admin-sdk.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "registry": "https://artifactory.corp.mongodb.com/artifactory/api/npm/mongodb-cloud-local/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build-main && yarn build-module",
    "build-main": "tsc -p tsconfig.cjs.json",
    "build-module": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "watch": "tsc -w -p tsconfig.esm.json",
    "eslint": "eslint 'src/**/*{js,ts}' 'test/**/*{js,ts}'",
    "lint": "yarn eslint",
    "lintfix": "yarn eslint --fix",
    "prettier": "prettier --write \"{,!(node_modules),!(dist)/**/}*.ts\""
  },
  "dependencies": {
    "@types/bson": "^4.0.2",
    "@types/jwt-decode": "^2.2.1",
    "bson": "4.0.4",
    "json2typescript": "^1.4.1",
    "jwt-decode": "^2.2.0",
    "query-string": "^6.12.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cross-fetch": "^3.0.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-replace-relative-imports": "^1.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "fetch-mock": "^9.10.1",
    "form-data": "^3.0.0",
    "jest": "^26.0.1",
    "md5": "^2.2.1",
    "mock-browser": "^0.92.14",
    "mongodb": "^3.5.8",
    "mongodb-stitch": "^3.16.0",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "ts-jest": "^26.0.0",
    "ts-sinon": "^1.2.0",
    "typescript": "^3.9.3"
  }
}
