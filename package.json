{
  "name": "mongodb-stitch",
  "version": "3.18.0",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "main": "dist/node/index.js",
  "scripts": {
    "build": "parcel build",
    "build-node": "parcel build --target node",
    "build-web": "parcel build --target web",
    "build-web-dev": "parcel build --target web --no-optimize",
    "dev": "parcel watch --target web-dev",
    "docs": "documentation build src/** -f html -o docs --config documentation.yml --theme ./docs_theme/ --github",
    "test": "jest",
    "lint": "eslint src/ test/",
    "lintfix": "eslint --fix src/ test/",
    "publish-docs": "npm run docs && gh-pages -d docs",
    "prepare": "npm run build"
  },
  "jest": {
    "automock": false,
    "testURL": "http://localhost/"
  },
  "targets": {
    "web-dev": {
      "source": "src/index.js",
      "context": "browser",
      "distDir": "./dist/web",
      "outputFormat": "esmodule",
      "scopeHoist": false
    },
    "web": {
      "source": "src/index.js",
      "context": "browser",
      "distDir": "./dist/web",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "node": {
      "source": "src/index.js",
      "context": "node",
      "distDir": "./dist/node",
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  },
  "dependencies": {
    "Base64": "^1.0.1",
    "bson": "^4.0.4",
    "detect-browser": "^1.7.0",
    "form-data": "^2.3.2",
    "jwt-decode": "^2.2.0",
    "node-fetch": "^2.6.0",
    "query-string": "^4.3.4"
  },
  "devDependencies": {
    "aws-sdk": "^2.222.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-version-transform": "^1.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chromedriver": "^83.0.0",
    "documentation": "^5.3.5",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.7.1",
    "express": "^4.16.2",
    "fetch-mock": "^5.10.1",
    "gh-pages": "^1.0.0",
    "jest": "^22.2.2",
    "jest-fetch-mock": "^1.7.5",
    "md5": "^2.2.1",
    "mock-browser": "^0.92.14",
    "mongodb": "^2.2.26",
    "parcel": "^2.0.1",
    "regenerator-runtime": "^0.10.5",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "sinon": "^2.2.0",
    "url-parse": "^1.1.9",
    "vinyl": "^2.1.0",
    "vinyl-fs": "^3.0.2",
    "webpack": "^2.5.1",
    "yargs": "^8.0.1"
  }
}
